{
    "name": "magrent",
    "license": "GPL 3.0",
    "author": "Siavash Askari Nasr",
    "version": "1.4.3",
    "fullName": "Magrent",
    "id": "jid1-9tZMAIdeuiEjHg@jetpack",
    "permissions": {
        "private-browsing": true,
        "multiprocess": true
    },
    "main": "lib/main.js",
    "description": "This add-on converts magnet links and torrent hashes to torrent files. It uses torrent storage services like Torrage.com for downloading torrent files.",
    "preferences": [
        {
            "name": "torrentSource",
            "type": "menulist",
            "title": "Default torrent source",
            "description": "Will be used in 'Download torrent file' context menu item",
            "value": "itorrents.org",
            "options": [
                {
                    "value": "itorrents.org",
                    "label": "iTorrents.org"
                },
                {
                    "value": "btcache.me",
                    "label": "btcache.me"
                },
                {
                    "value": "torrasave.top",
                    "label": "Torrasave.top"
                },
                {
                    "value": "thetorrent.org",
                    "label": "TheTorrent.org"
                }
            ]
        }
    ]
}
